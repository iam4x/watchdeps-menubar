{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "plugin": "eslint-plugin-react",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "jsx-quotes": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "no-console": 0
  }
}
